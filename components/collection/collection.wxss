.songList {
  padding: 30rpx 30rpx 20rpx 30rpx;
  background-color: #ffffff;
}

.ges {
  display: flex;
  align-items: center;
  padding: 15rpx 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.albumImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 20rpx;
}

.album {
  margin-left: 30rpx;
}

.albumName {
  font-size: 28rpx;
  color: #333333;
}

.albumItem {
  font-size: 24rpx;
  color: #999999;
  display: flex;
  align-items: center;
  padding: 4rpx 0;
}

.albumItem>view {
  margin-right: 20rpx;
}