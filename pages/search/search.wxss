.search{
  display: flex;
  align-items: center;
}
.input{
  width: 680rpx;
}
.cancel{
  font-size: 28rpx;
}
.connet{
  padding: 20rpx;
}
.connet1{
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #f0f0f0;
  padding-bottom: 20rpx;
}
.connet1>view:nth-of-type(1){
  font-size: 28rpx;
}
.connet1>view:nth-of-type(2){
  font-size: 22rpx;
  display: flex;
  align-items: center;
  border-radius: 21rpx;
  border: 2rpx solid #f0f0f0;
  padding: 4rpx 14rpx;
}
/* 搜索列表 */
.searchList{
display: flex;
flex-wrap: wrap;
margin-left: 30rpx;
}
.searchLista{
  width: 50%;
  padding: 16rpx 0;
  font-size: 26rpx;
  color: #333333;
}
.searchLista:hover{
  background-color: #f5f5f5;
}
.searchLista>text:nth-of-type(1){
color: #999999;
margin-right: 20rpx;
}
.box{
  color: red !important;
  font-weight: bold;
}
.boxa{
  font-weight: bold;
}
.hot{
  color: red;
  font-size: 18rpx;
font-weight: 550;
margin-left: 10rpx;
font-style: oblique;
}
.more{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  color: #999999;
  margin: 20rpx 0;
}
.more>text{
  padding-right: 8rpx;
}
.arrows{
font-size: 28rpx;
font-weight: 900;
color: red;
margin-left: 10rpx;
}
.imagea1{
  width: 100%;
  height:930rpx;
}
.searchTerms{
  font-size: 26rpx;
  color: #658CBA;
  padding: 0 0 30rpx 30rpx;
  border-bottom: 2rpx solid #F6F7F8;
}
.keyword{
  font-size: 20rpx !important;
  color: #333333;
}
.historyList{
  display: flex;
  margin:30rpx;
  position: relative;
}
.history{
  display: flex;
}
.historya{
  font-size: 28rpx;
  color: #333333;
  font-weight: bold;
  margin-right: 30rpx;
}
.historyb{
  font-size: 24rpx;
  padding: 4px 20rpx;
  background-color: #f5f5f5;
  border-radius: 30rpx;
  margin-right: 30rpx;
}
.historyc{
  position: absolute;
  top: 0;
  right: 0;
}
.fiexdPlay{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}